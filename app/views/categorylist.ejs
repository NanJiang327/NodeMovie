<%- include('components/header.ejs') %>
    <div class="container">
        <div class="row">
            <table class="table table-hover table-bordered">
                <thead>
                    <tr>
                        <th>Category</th>
                        <th>Time</th>
                        <th>View</th>
                        <th>Update</th>
                        <th>More</th>
                    </tr>
                    <tbody>
                        <% category.forEach(function (item){ %>
                           <tr class="item-id-<%= item._id %>">
                               <td><%= item.name %></td>
                               <td><%= moment(item.meta.updateAt).format('MM/DD/YYYY') %></td>
                               <td><a href="/" target="_blank">Detail</a></td>
                               <td><a href="/" target="_blank">Update</a></td>
                               <td>
                                   <button class="btn btn-danger delList" type="button" data-id="<%= item._id %>">Delete</button>
                                </td>
                           </tr>
                        <% }) %>
                    </tbody>
                </thead>
            </table>
        </div>
    </div>
    <script src="/js/admin.js"></script>
<%- include('components/footer.ejs') %>
